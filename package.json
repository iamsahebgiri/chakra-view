    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.2.0",
    "@types/react": "^17.0.32",
    "commitlint": "^13.2.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-sznm": "^0.1.10",
