{
  "name": "chakra-view",
  "version": "0.1.0",
  "private": true,
  "author": "Saheb Giri",
  "description": "Over 30+ professionally designed and fully responsive Chakra UI compoents.",
  "scripts": {
    "dev": "next dev",
